SPKID = "3542519"  # example SPK-ID for an asteroid
app = Flask(__name__)
@app.route('/')
def index():
    url = "https://api.nasa.gov/neo/rest/v1/neo/" + SPKID + "?api_key=WLnxw2BlieP9zAAfu9MHwN5NwZOhytUmRVjBzTcW"
    response = requests.get(url)
    data = response.json()
    #pprint.pprint(data['near_earth_objects'])
    #print(len(data['near_earth_objects'][0]['close_approach_data'][0]))
    return data

index(SPKID)

